    "@reliverse/fs": "^1.0.0",
    "@reliverse/prompts": "^1.6.0",
    "@reliverse/relico": "^1.0.1",
    "@reliverse/runtime": "^1.0.2",
    "@sinclair/typebox": "^0.34.31",
    "@vercel/sdk": "^1.5.0",
    "ai": "^4.2.3",
    "async-listen": "^3.1.0",
    "better-sqlite3": "^11.9.1",
    "bun-types": "^1.2.6",
    "confbox": "^0.2.1",
    "destr": "^2.0.3",
    "detect-package-manager": "^3.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "escape-string-regexp": "^5.0.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "https-proxy-agent": "^7.0.6",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "jsonrepair": "^3.12.0",
    "lookpath": "^1.2.3",
    "magic-string": "^0.30.17",
    "magicast": "^0.3.5",
    "nanoid": "^5.1.5",
