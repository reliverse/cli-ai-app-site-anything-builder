{
  "name": "@reliverse/cli",
  "author": "reliverse",
  "version": "1.6.5",
  "type": "module",
  "license": "MIT",
  "keywords": ["cli"],
  "description": "@reliverse/cli is your all-in-one companion for building and improving web projects â€” whether you're kicking off something new or upgrading an existing app. It's like having a little AI-powered toolbox in your terminal, ready to help with coding, refactoring, image gen, and more.",
  "scripts": {
    "dev": "bun dev:cli",
    "pub": "relidler",
    "knip": "knip",
    "vi": "vitest",
    "test": "bun cli-test.ts",
    "db": "drizzle-kit studio",
    "latest": "bun update --latest && bun check",
    "start": "bun src/main.ts cli --cwd start-runtime",
    "check": "tsc --noEmit && eslint --cache --fix . && biome check --write .",
    "agg:cfg": "relidler tools --tool agg --input src/libs/cfg/constants --out src/libs/cfg/cfg-main.ts --recursive --named --strip src/libs/cfg",
    "agg:sdk": "relidler tools --tool agg --input src/libs/sdk/constants --out src/libs/sdk/sdk-main.ts --recursive --named --strip src/libs/sdk",
    "agg": "bun agg:cfg && bun agg:sdk",
    "dev:cli": "bun src/main.ts cli --dev",
    "dev:add": "bun src/main.ts add --dev",
    "dev:ai": "bun src/main.ts ai --dev",
    "dev:clone": "bun src/main.ts clone --dev",
    "dev:cmod": "bun src/main.ts cmod --dev",
    "dev:env": "bun src/main.ts env --dev",
    "dev:help": "bun src/main.ts help --dev",
    "dev:init": "bun src/main.ts init --dev",
    "dev:login": "bun src/main.ts login --dev",
    "dev:logout": "bun src/main.ts logout --dev",
    "dev:memory": "bun src/main.ts memory --dev",
    "dev:multireli": "bun src/main.ts multireli --dev",
    "dev:schema": "bun src/main.ts schema --dev",
    "dev:studio": "bun src/main.ts studio --dev",
    "dev:toolbox": "bun src/main.ts toolbox --dev",
    "dev:update": "bun src/main.ts update --dev",
    "dev:upload": "bun src/main.ts upload --dev"
  },
