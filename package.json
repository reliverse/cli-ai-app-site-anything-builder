    "p-map": "^7.0.3",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "pretty-bytes": "^6.1.1",
    "querystring": "^0.2.1",
    "random-words": "^2.0.1",
    "semver": "^7.7.1",
    "simple-git": "^3.27.0",
    "tar": "^7.4.3",
